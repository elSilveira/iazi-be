{
  "name": "iazi-api",
  "version": 2,
  "buildCommand": "powershell -c \"& .\\railway-build.bat\"",
  "startCommand": "npx prisma generate && node dist/index.js",
  "healthcheckPath": "/api/health",
  "healthcheckTimeout": 15,
  "restartPolicyType": "ON_FAILURE",
  "restartPolicyMaxRetries": 5,
  "environments": {
    "production": {
      "healthcheck": {
        "path": "/api/health",
        "timeout": 15
      },
      "restartPolicyType": "ON_FAILURE",
      "restartPolicyMaxRetries": 5
    }
  }
}
